.el-table__footer {
    font-weight: bold
}
.conAchieve .el-table__body .cell {
     padding-left: 0 !important; 
     padding-right: 0 !important; 
    box-sizing: border-box;
    text-overflow: ellipsis;
}

.conAchieve .cell .append{
    padding-left: 13px; 
    padding-right: 13px; 
    border-bottom: 1px solid #eef1f6;
    line-height: 40px
}
.conAchieve .cell .append:last-child{
    border-bottom:none
}

