.selCommon .nav {
    position: relative;
    height: 36px;
    margin-bottom: 10px;
    line-height: 36px;
}

/*菜单颜色修改*/
.selCommon .el-form-item__error {
    color: #ff4949;
    font-size: 12px;
    // line-height: 3 !important;
    padding-top: 4px;
    position: static !important;
    top: 100%;
    left: 0;
}

.selCommon {
    .el-input,
    .el-select {
        float: left;
        // margin-right: 5px;
    }
}

/*表格样式修改*/

.selCommon .commodityAudit .el-table--border td {
    border-right: none !important;
}

.selCommon .commodityAudit .el-table--border tr td:last-child {
    border-right: 1px solid #eef1f6 !important;
}

.selCommon .backB {
    background: #DCF0F9
}

/*表格样式修改*/

 .el-radio__label {
    font-size: 12px !important;
    padding-left: 5px;
}

.selCommon .my-autocomplete {
    li {
        line-height: normal;
        padding: 7px;
        .name {
            text-overflow: ellipsis;
            overflow: hidden;
        }
        .addr {
            font-size: 12px;
            color: #b4b4b4;
        }
        .highlighted .addr {
            color: #ddd;
        }
    }
    
}
.el-table__fixed{//table固定列在滑动条时底部无法对齐
    bottom:0px !important;
}
.selCommon .el-autocomplete .el-input{
    float:none;
}

.selCommon .self_ipt {
    -webkit-appearance: none;
    -moz-appearance: none;
    // appearance: none;
    -webkit-appearance: none;
    background-color: #fff;
    background-image: none;
    border-radius: 4px;
    border: 1px solid #bfcbd9;
    box-sizing: border-box;
    color: #1f2d3d;
    font-size: inherit;
    height: 36px;
    line-height: 1;
    outline: 0;
    padding: 3px 10px;
}

.selCommon .el-tabs__item {
    font-size: 12px !important;
}

.selCommon .block {
    float: right;
}


.selCommon {
    .el-checkbox__label {
        font-size: 12px !important;
    } 
    //搜索栏时间
    .date_picker_sear {
        width: 150px;
    } //搜索栏输入框
    .form_sear_ipt {
        width: 200px;
    }
    .form_sear_ipt_half {
        width: 80px;
    }
    
    .form_item_full {
        width: 100%;
    }
    .form_item_half {
        width: 48%;
    }
    .wt200 {
        width: 200px;
    }
    .num_right {
        float: right;
        margin-right: 10px;
    }
    .footer_ope {
        text-align: center;
        margin-top: 10px;
    }
    .table_container {
        min-height: 260px;
        max-height: 460px;
        height: auto;
    }
    .avatar-uploader .el-upload {
        border: 1px dashed #d9d9d9;
        border-radius: 6px;
        cursor: pointer;
        position: relative;
        overflow: hidden;
    }
    .avatar-uploader .el-upload:hover {
        border-color: #20a0ff;
    }
    .el-table__expanded-cell {
        padding: 0px 20px !important;
    }
}

.el-table__fixed-header-wrapper thead div{
    text-align: center !important;
}


.newNotice #quill-container {
    height: 210px !important;
}
.projectKnowledge .ql-snow .ql-editor blockquote {
    border-left: 4px solid #FF664F;
    margin-bottom: 5px;
    margin-top: 5px;
    padding-left: 16px;
}
